  "paths": {
    "/api/auth/": {
      "get": {
        "security": [{
            "apiKeyAuth": []
        }],
        "description": "Get current authenticated user",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "_id": {
                      "type": "string"
                    },
                    "email": {
                      "type": "string"
                    },
                    "date": {
                      "type": "string"
                    }
                  }
                },
                "example": {
                  "_id": "5f9e3b7b9c9d440000d9d4b0",
                  "email": "email@adopt.local",
                  "date": "2020-11-01T18:00:00.000Z"
                }
              }
            }
          }
        }
      },
      "post": {
        "description": "Login user",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "token": {
                      "type": "string"
                    }
                  }
                },
                "example": {
                  "token": "JWT_TOKEN"
                }
              }
            }
          }
        },
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "email": {
                    "example": "string"
                  },
                  "password": {
                    "example": "string"
                  }
                }
              },
              "example": {
                "email": "email@adopt.local",
                "password": "123456"
              }
            }
          }
        }
      }
    },
